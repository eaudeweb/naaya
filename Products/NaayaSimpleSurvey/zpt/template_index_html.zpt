<metal:block use-macro="here/standard_template_macro">
<metal:block fill-slot="body">
<style type="text/css">
table {width: 100%; border:1px solid black; table-layout: auto; border-collapse: collapse;}
table td {border:1px solid black}
table th {border:1px solid black}
colgroup.column1 {width: 5%;}
colgroup.column2 {width: 45%;}
colgroup.column3 {width: 10%;}
colgroup.column4 {width: 8%;}
.none {background-color: black; color: white; display: block;}
.minor {background-color: #9FFF9F; color: green; display: block;}
.medium {background-color: green; color: white; display: block;}
.major {background-color: red; color: white; display: block;}
caption {font-size: larger; border: 1px solid black; font-weight: bold; text-align: center;}
th {font-weight: bold; background-color: #FFFF84;}
textarea {width: 98%;}
</style>
<h1 tal:content="here/title_or_id" />

<p i18n:translate="" tal:condition="not:python:here.check_template_deadline()">Cannot fill in the answers since the deadline for this survey has passed.
</p>
<tal:block tal:condition="python:here.check_template_deadline() or here.checkPermissionPublishObjects()">
<p i18n:translate="" tal:condition="python:here.check_if_answered(request)">You already answered this survey.</p>
<tal:block tal:condition="python:not(here.check_if_answered(request)) or here.checkPermissionPublishObjects()" tal:define="ses python:here.REQUEST.SESSION">
<p i18n:translate="">Please fill in the appropriate boxes, then click the <em i18n:name="submit_answer" i18n:translate="">Submit answer</em> button.<br i18n:name="br"/>
<strong i18n:name="note" i18n:translate="">NOTE:</strong> You can answer this survey only once.<br i18n:name="br"/>
<strong i18n:name="tip" i18n:translate="">TIP:</strong> If you wish to print this page before submitting make sure you specify the <em i18n:name="landscape" i18n:translate="">landscape</em> 
printing mode(so all page content is included).</p>

<form id="survey" method="post" action="addNySurveyAnswer" tal:define="drp_opt python:['none', 'Regulation', 'Planning', 'Guidance', 'Incentive', 'Monitoring', 'Incentive payments', 'Advice', 'Management', 'Habitat', 'Other - specify']">

<label for="survey_contributor" i18n:translate="">Your name:</label>
<input type="text" name="survey_contributor" id="survey_contributor" tal:attributes="value python:here.getAuthenticationTool().getUserFullName(here.REQUEST.AUTHENTICATED_USER)" />

<label for="country" i18n:translate="">Country:</label>
<input id="country" name="country" type="text" size="20" tal:attributes="value python:here.REQUEST.SESSION['country']" tal:condition="python:here.REQUEST.SESSION.has_key('country')" />
<input id="country" name="country" type="text" size="20" tal:condition="not:python:here.REQUEST.SESSION.has_key('country')" />

<label for="agency" i18n:translate="">Name of the Agency:</label>
<input id="agency" name="agency" type="text" size="20" tal:attributes="value python:here.REQUEST.SESSION['agency']" tal:condition="python:here.REQUEST.SESSION.has_key('agency')" />
<input id="agency" name="agency" type="text" size="20" tal:condition="not:python:here.REQUEST.SESSION.has_key('agency')" />
<br />
<br />


<table summary="">
