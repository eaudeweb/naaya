			<td><label for="atr" tal:attributes="for python:'%sP' % atr">Primary</label><br /><select tal:attributes="id python:'%sP' % atr; name python:'%sP' % atr"><option tal:repeat="opt drp_opt" tal:content="opt" tal:attributes="value opt;selected python:ses.get('%sP' % atr, '') == opt" /></select></td>
			<td><label for="atr" tal:attributes="for python:'%sS' % atr">Secondary</label><br /><select tal:attributes="id python:'%sS' % atr; name python:'%sS' % atr"><option tal:repeat="opt drp_opt" tal:content="opt" tal:attributes="value opt;selected python:ses.get('%sS' % atr, '') == opt" /></select></td>
