			<item id="Egypt" title="Egypt"/>
			<item id="France" title="France"/>
			<item id="Greece" title="Greece"/>
			<item id="Israel" title="Israel"/>
			<item id="Italy" title="Italy"/>
			<item id="Jordan" title="Jordan"/>
			<item id="Lebanon" title="Lebanon"/>
			<item id="Lybia" title="Lybia"/>
			<item id="Malta" title="Malta"/>
			<item id="Morocco" title="Morocco"/>
			<item id="Palestinian Authority" title="Palestinian Authority"/>
