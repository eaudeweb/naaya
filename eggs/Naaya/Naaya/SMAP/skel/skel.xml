<?xml version="1.0" encoding="utf-8"?>
<skel>
	<forms>
		<form id="folder_index" title="NyFolder default view"/>
		<form id="site_index" title="Portal home page"/>
		<form id="site_login" title="Portal login"/>
		<form id="site_logout" title="Portal logout"/>
	</forms>
	<layout default_skin_id="smap" default_scheme_id="blue">
		<skin id="smap" title="SMAP">
			<template id="site_header" title="Portal standard HTML header"/>
			<template id="site_footer" title="Portal standard HTML footer"/>
			<template id="portlet_left_macro" title="Macro for left portlets"/>
			<template id="portlet_center_macro" title="Macro for center portlets"/>
			<template id="portlet_right_macro" title="Macro for right portlets"/>
			<scheme id="blue" title="Blue">
				<image id="bck_li.gif" title=""/>
				<image id="bckg_left.jpg" title=""/>
				<style id="style" title="Main CSS"/>
				<style id="style_rtl" title="Main CSS for RTL"/>
			</scheme>
		</skin>
	</layout>
	<pluggablecontenttypes>
		<pluggablecontenttype meta_type="Naaya Media File" action="1"/>
		<pluggablecontenttype meta_type="Naaya File" action="0"/>
		<pluggablecontenttype meta_type="Naaya Extended File" action="1"/>
		<pluggablecontenttype meta_type="Naaya SMAP Project" action="1"/>
		<pluggablecontenttype meta_type="Naaya SMAP Expert" action="1"/>
	</pluggablecontenttypes>
	<portlets left="portlet_maincategories" center="portlet_latestuploads_rdf">
		<portlet id="portlet_rdfcalendar" title="RDF Calendar"/>
		<portlet id="portlet_administration" title="Administration" />
		<reflist id="countries" title="Countries" description="List of countries used by Naaya SMAP Project objects. When adding or editing projects, the end users will be able to choose from list below.">
			<item id="Albania" title="Albania"/>
