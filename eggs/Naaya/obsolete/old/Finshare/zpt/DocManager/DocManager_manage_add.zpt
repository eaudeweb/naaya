    <tr>
        <th style="text-align: left;">Title</th>
        <td><input type="text" name="title" size="50"></td>
    </tr>
	<tr>
        <th valign="top" style="text-align: left;">Description</th>
        <td><textarea name="description" rows="5" cols="50"></textarea></td>
    </tr>
    <tr>
        <th style="text-align: left;">Webmaster email</th>
        <td><input type="text" name="webmaster" size="35"></td>
