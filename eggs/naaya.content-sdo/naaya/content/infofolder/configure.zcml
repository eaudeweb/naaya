<configure xmlns="http://namespaces.zope.org/zope"
	xmlns:naaya="http://namespaces.zope.org/naaya"
	xmlns:five="http://namespaces.zope.org/five"
	i18n_domain="naaya.content">

	<naaya:content factory=".InfoFolder.get_config" />
	<five:registerPackage
		package="."
		initialize=".initialize"/>

	<adapter factory="naaya.content.infofolder.portlets.InfoFolderPortlet" name="latest_uploads_portlet" />
	<adapter factory="naaya.content.infofolder.portlets.InfoFolderSearchPortlet" name="infofolder_search_portlet" />
	<adapter factory="naaya.content.infofolder.portlets.InfoFolderEventsPortlet" name="events_filter" />
	<adapter factory="naaya.content.infofolder.portlets.InfoFolderSubmitSitePortlet" name="submit_site_portlet" />
</configure>


