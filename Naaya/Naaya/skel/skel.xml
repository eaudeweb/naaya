			permissions="Naaya - Add comments for content"/>
		<grouppermissions name="Edit content" description="Adding, publishing and deleting are not granted as a part of this privilege. Edit content is only allowed for owners if users do not have the 'Naaya - Publish content' permission."
			permissions="Naaya - Access administrative area,Naaya - Edit content,Take ownership"/>
		<grouppermissions name="Copy content" description="Copy content. Adding, publishing and deleting are not granted as a part of this privilege. In order to be able to paste the copied content, users must have the add permission in the target location."
			permissions="Naaya - Copy content,Take ownership"/>
		<grouppermissions name="Publish content" description="Publish Naaya objects. Adding, editing and deleting are not granted as a part of this privilege."
			permissions="Naaya - Access administrative area,Naaya - Publish content,Take ownership"/>
		<grouppermissions name="Delete content" description="Delete Naaya objects. Adding, publishing and editing are not granted as a part of this privilege. Delete content is only allowed for owners." 
			permissions="Naaya - Access administrative area,Naaya - Delete content,Take ownership,Naaya - Manage comments for content"/>
		<grouppermissions name="Validate content" description="Validate Naaya objects. Adding, editing and deleting are not granted as a part of this privilege."
			permissions="Naaya - Access administrative area,Naaya - Validate content,Take ownership"/>
		<grouppermissions name="Translate pages" description="Translate pages in the portal"
			permissions="Naaya - Access administrative area,Naaya - Translate pages"/>
		<grouppermissions name="Manage Users" description="Create/modify/delete users in the portal"
			permissions="Naaya - Access administrative area,Manage users"/>
		<role name="Anonymous" grouppermissions="Browse content" permissions=""/>
		<role name="Authenticated" grouppermissions="Add comments" permissions=""/>
		<role name="Contributor" grouppermissions="Add content" permissions=""/>
		<role name="Administrator" grouppermissions="Browse content,Add content,Add comments,Edit content,Copy content,Publish content,Delete content,Validate content,Translate pages,Manage Users" permissions=""/>
