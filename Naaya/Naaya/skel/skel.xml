	<security>
		<grouppermissions name="Browse content" description="Browse the site structure. This is the base privilege aimed for Anonymous users."
			permissions="Access Transient Objects,Access contents information,Access session data,Query Vocabulary,Search ZCatalog,Use Database Methods,Use mailhost services,View,WebDAV access,WebDAV Unlock items,WebDAV Lock items"/>
